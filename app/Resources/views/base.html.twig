<script>
    // Select materializecss
    $(document).ready(function() {
        $('select').material_select();
    });

    $(window).scroll(function(event){
        var st = $(this).scrollTop();
        var boundary = 10;

        if (st > boundary) {
            $('header').addClass('active');
        } else{
            $('header').removeClass('active');
        }
    });

</script>

