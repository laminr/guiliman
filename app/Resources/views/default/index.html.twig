{% extends 'base.html.twig' %}

{% block body %}
    {{ include('::header.html.twig') }}

    <div class="container">
        <div class="row">
            <div class="col s12 m6 l8">
                <h2>Message du Pr&eacute;sident</h2>
            </div>
            <div class="col s12 m6 l4">
                <h3>Consultations</h3>
                {% for dto in dtos %}
                    <div class="row poll-item">
                        <div class="question-title-sum">
                            <a href="{{ path('poll_index', { 'id' : dto.question.id }) }}" class="link">
                                {{ dto.question.label }}
                                <span class="right"> > </span>
                            </a>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>

    </div>
{% endblock %}

